            <br>
            <button type="submit" class="btn btn-primary" >Conectar</button>
            <button class ="btn btn-defauld"style="margin-top: 10px;" type="button" value="voltar" onclick="history.go(-1)">Voltar</button>
        </form>
        <script src="/java/funcoes.js"></script>
    </div>
  </div>
